<?xml version ="1.0"?>
<odoo>

    <record id="medical_history_view_tree" model="ir.ui.view">
        <field name = "name">medical.history.tree</field>
        <field name = "model">medical.history</field>
        <field name = "arch" type="xml">
            <tree editable = "bottom">
                <field name = "name"/>
                <field name = "start_date"/>
                <field name = "end_date"/>
                <field name = "sequelae"/>
                <field name = "note"/>
            </tree>
        
        </field>
    </record>
</odoo>
